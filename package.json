{
  "name": "se_m2_be",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "tsc:w": "node ./node_modules/typescript/bin/tsc -w",
    "start": "node ./build/index.js",
    "start:dev": "nodemon ./build/index.js",
    "test": "jest --force-exit",
    "coverage": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LucaStefan112/SE_M2_BE.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.6",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts, tsx}",
      "!src/**/*.d.ts",
      "!src/methods/jsonCreator.ts",
      "!src/controllers/appointments.controller.ts"
    ]
  },
  "dependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express-rate-limit": "^6.7.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.1",
    "ts-node": "^10.9.1",
    "zod": "^3.21.4"
  }
}
